generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                 String   @id @unique
  subscribedChannels String[]

  shipments String?
  viewerUrl String?
  apiUrl String?
}

enum ShipmentType {
  WarehouseShipment
  HSHQMailShipment
  HSRawPendingAGHShipment
  HS3rdPartyPhysicalShipment
  HSMinutemanShipment
  BobaDropsShipment
  SprigShipment
  Other
}
